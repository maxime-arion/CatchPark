<h1>Se garer</h1>

<%= form_with model: @parking, local: true do |form| %>
  <%= form.label :duration %>
  <%= form.text_field :duration %>

  <%= form.label :address %>
  <%= form.text_field :address %>

  <%= form.label :status %>
  <%= form.check_box :status %>

  <%= form.submit 'Valider' %>
<% end %>

<%= link_to 'Retour', parking_path(@parking) %>
